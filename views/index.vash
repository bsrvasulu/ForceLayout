<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="d3.js based force layout">
        <meta name="author" content="Sreeni">

        <title>Force Layout</title>
        <!-- Bootstrap Core CSS -->
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
        <link href="/css/nodeLinkStyle.css" rel="stylesheet">
    </head>

    <body>
        <div id="wrapper" class="container-fluid">

            <!-- Navigation -->
            <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
                <div class="navbar-header">
                    <a class="navbar-brand" href="#/">Force Layout - Site Viewer</a>
                </div>
            </nav>

            <div id="page-wrapper">
                <div id="testid">
                    <div class="row">
                        <div class="col-lg-9">
                            <section id="vis" ></section>
                        </div>
                        <div class="col-lg-3">
                            <div class="form-group">
                                <button id="forcelayout" class="btn btn-primary" onclick="loadForceLayout()">Load Force Layout</button>
                            </div>
                            <div class="form-group">
                                <button id="forcelayoutSME" class="btn btn-primary" onclick="getSMELinks()">Show SME Links</button>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <!-- /#page-wrapper -->

        </div>
        <!-- /#wrapper -->

        <!-- jQuery -->
        <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
        <script src="http://d3js.org/d3.v3.min.js" charset="utf-8"></script>
        <script src="/js/servicejs.js"></script>
        <script src="/js/loadForceLayout.js"></script>
    </body>

</html>
